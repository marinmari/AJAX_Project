
if (document.getElementById("email-body-<%=@email.id%>").children.length === 0){
  $("#email-list-<%=@email.id%>").html("<%= j render 'emailshow', email: @email %>").children(':last').hide().fadeIn(2000);
}else{
  $("#email-list-<%=@email.id%>").html("<%= j render 'emailshow2', email: @email %>").children(':last').hide().fadeIn(2000);}
